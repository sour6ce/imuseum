{
  "version": "2.0.0",
  "tasks": [
    {
      "label": "build",
      "command": "dotnet",
      "type": "process",
      "args": [
        "build",
        "${workspaceFolder}/build.proj",
        "/property:GenerateFullPaths=true",
        "/consoleloggerparameters:NoSummary"
      ],
      "problemMatcher": "$msCompile"
    },
    {
      "label": "publish",
      "command": "dotnet",
      "type": "process",
      "args": [
        "publish",
        "${workspaceFolder}/IMuseum.Presentation/IMuseum.Presentation.csproj",
        "/property:GenerateFullPaths=true",
        "/consoleloggerparameters:NoSummary"
      ],
      "problemMatcher": "$msCompile"
    },
    {
      "label": "watch",
      "command": "dotnet",
      "type": "process",
      "args": [
        "watch",
        "run",
        "--project",
        "${workspaceFolder}/IMuseum.Presentation/IMuseum.Presentation.csproj"
      ],
      "problemMatcher": "$msCompile"
    },
    {
      "label": "migrate task",
      "command": "dotnet",
      "type": "process",
      "args": [
        "ef",
        "migrations",
        "add",
        "Automated",
        "--project",
        "${workspaceFolder}/IMuseum.Persistence/IMuseum.Persistence.csproj",
        "--startup-project",
        "${workspaceFolder}/IMuseum.Presentation/IMuseum.Presentation.csproj"
      ],
      "problemMatcher": []
    },
    {
      "label": "datbase update task",
      "command": "dotnet",
      "type": "process",
      "args": [
        "ef",
        "database",
        "update",
        "--project",
        "${workspaceFolder}/IMuseum.Persistence/IMuseum.Persistence.csproj",
        "--startup-project",
        "${workspaceFolder}/IMuseum.Presentation/IMuseum.Presentation.csproj"
      ],
      "problemMatcher": []
    }
  ]
}